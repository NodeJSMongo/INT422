@model IEnumerable<Assignment8.Controllers.GenreBase>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                <a href="http://www.allmusic.com/genres" target="_blank">allmusic.com</a>
            </td>
        </tr>
    }

</table>